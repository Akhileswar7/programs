#swap 3 numbers
a=int(input("Enter num1"))
b=int(input("Enter num2"))
c=int(input("Enter num3"))
if a < b:
    if b < c:
        print (a,b,c)
    else:
        if a < c:
            print (a,c, b)
        else:
            print (c,a,b)
else:
    if c < b:
        print (c,b,a)
    else:
        if c < a:
            print (b,c,a)
        else:
            print (b,a,c)







