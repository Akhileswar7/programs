#Palindrome
num=int(input("Enter num"))
num1=num
rnum=0
while(num>0):
    r=num%10
    rnum=rnum*10+r
    num=num//10
if(num1==rnum):
    print(num1,"is a palindrome")
else:
    print(num1,"is a not palindrome")
    
